<?xml version="1.0"?>
<tool_dependency>
    <package name="python" version="2.7.10">
        <repository name="package_python_2_7_10" owner="iuc" prior_installation_required="True" />
    </package>
    <package name="fisher" version="0.1.4">
        <repository name="package_python_2_7_fisher_0_1_4" owner="bebatut" prior_installation_required="True" />
    </package>
    <package name="statsmodels" version="0.6.1">
        <repository name="package_python_2_7_statsmodels_0_6_1" owner="bebatut" prior_installation_required="True" />
    </package>
    <package name="xlsxwriter" version="0.8.5">
        <repository name="package_python_2_7_xlsxwriter_0_8_5" owner="bebatut" prior_installation_required="True" />
    </package>
    <package name="wget" version="3.2">
        <repository name="package_python_2_7_wget_3_2" owner="bebatut" prior_installation_required="True" />
    </package>
    <package name="goatools" version="0.6.4">
        <install version="1.0">
            <actions>
                <action type="download_by_url">https://github.com/tanghaibao/goatools/archive/v0.6.4.zip</action>
                <action type="move_directory_files">
                    <source_directory>.</source_directory>
                    <destination_directory>$INSTALL_DIR/</destination_directory>
                </action>

                <action type="setup_python_environment">
                    <repository name="package_python_2_7_10" owner="iuc">
                       <package name="python" version="2.7.10" />
                    </repository>
                    <repository name="package_python_2_7_fisher_0_1_4" owner="bebatut">
                        <package name="fisher" version="0.1.4" />
                    </repository>
                    <repository name="package_python_2_7_statsmodels_0_6_1" owner="bebatut">
                        <package name="statsmodels" version="0.6.1" />
                    </repository>
                    <repository name="package_python_2_7_xlsxwriter_0_8_5" owner="bebatut">
                        <package name="xlsxwriter" version="0.8.5" />
                    </repository>
                    <repository name="package_python_2_7_wget_3_2" owner="bebatut">
                        <package name="wget" version="3.2" />
                    </repository>

                    <package md5sum="2d5f1f9319dbf59850bf92cfa71e92ee">https://pypi.python.org/packages/source/g/goatools/goatools-0.5.9.tar.gz</package>
                </action>

                <action type="set_environment">
                    <environment_variable action="append_to" name="PYTHONPATH">$INSTALL_DIR</environment_variable>
                    <environment_variable action="set_to" name="PYTHONPATH_GOATOOLS">$INSTALL_DIR</environment_variable>
                    <environment_variable action="set_to" name="GOATOOLS_ROOT_PATH">$INSTALL_DIR</environment_variable>
                    <environment_variable name="GOATOOLS_DIR" action="set_to">$INSTALL_DIR/</environment_variable>
                </action>
            </actions>
        </install>
        <readme></readme>
    </package>
</tool_dependency>